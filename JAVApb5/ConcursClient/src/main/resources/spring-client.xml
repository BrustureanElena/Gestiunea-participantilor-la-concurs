<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

    <bean id="concursService" class="org.springframework.remoting.rmi.RmiProxyFactoryBean">
        <property name="serviceUrl" value="rmi://192.168.0.102:1099/Concurs"/>
        <!-- <property name="serviceUrl" value="rmi://localhost:1099/Chat"/>-->
        <property name="serviceInterface" value="concurs.service.IConcursService"/>
    </bean>
</beans>